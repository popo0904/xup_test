# CLK source 100 MHz
set_property PACKAGE_PIN Y9 [get_ports clk_pin]
set_property IOSTANDARD LVCMOS33 [get_ports clk_pin]

# BTNU
set_property PACKAGE_PIN T18 [get_ports btn_pin]
